RewriteEngine On
RewriteRule ^([0-9]+)/([0-9]+)/?$ ./month.php?y=$1&m=$2 [L]
RewriteRule ^([0-9]+)/([0-9]+)/([0-9]+)/?$ ./day.php?y=$1&m=$2&d=$3 [L]
RewriteRule ^favs/([0-9]+)/([0-9]+)/?$ ./fav_month.php?y=$1&m=$2 [L]
RewriteRule ^favs/([0-9]+)/([0-9]+)/([0-9]+)/?$ ./fav_day.php?y=$1&m=$2&d=$3 [L]
RewriteRule ^sort/?$ ./sort.php [L]
RewriteRule ^favorites/?$ ./favorites.php [L]
RewriteRule ^favs/?$ ./fav_index.php [L]
RewriteRule ^search/?$ ./search.php [L]
RewriteRule ^searchfeed/?$ ./searchfeed.php [L]